<header class="header">
    <div class="logo">
      <img src="../../../public/draw10Logo.png" alt="Logo" />
    </div>
    <div class="site-name">
      <p>Draw10</p>
    </div>
    <div id="log-button" class="log-button">
      <% if (user.isLoggedIn) { %>
        <button class="btn">Profile</button>
        <button class="btn">Log Out</button>
      <% } else { %>
        <button class="btn">Log In</button>
        <button class="btn">Sign Up</button>
      <% } %>
    </div>
  </header>